{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --build && tsc-alias",
    "prestart": "npm run build",
    "start": "node ./dist/src/index.js",
    "predev": "npm run build && npm run biome:check",
    "dev": "tsx --watch src/index.ts | pino-pretty",
    "biome:check": "npx @biomejs/biome check --write ./src",
    "db:generate": "npx drizzle-kit generate --name=$npm_config_name",
    "db:seed": "npx drizzle-kit generate --custom --name=$npm_config_name",
    "db:migrate": "npx drizzle-kit migrate",
    "db:studio": "npx drizzle-kit studio --port 4000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/nodemailer": "^7.0.1",
    "drizzle-kit": "^0.31.4",
    "pino-pretty": "^13.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "envalid": "^8.1.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "nodemailer": "^7.0.6",
    "pino": "^9.11.0"
  }
}
